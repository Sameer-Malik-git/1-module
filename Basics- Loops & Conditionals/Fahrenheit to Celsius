import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		//your code here
        Scanner in = new Scanner(System.in);
        int tempInFah = in.nextInt();
        int tempInCel = ((tempInFah - 32)*5)/9;
        System.out.println(tempInCel);
	}
}


